  "scripts": {
    "update": "pnpm i gatsby@latest --legacy--peer-deps && pnpm outdated",
    "reinstall": "rm -r ./node_modules && rm package-lock.json && rm pnpm-lock.yaml && pnpm i && pnpm run clean",
    "preload-fonts": "set LOG_LEVEL=info & gatsby-preload-fonts",
    "debug-fonts": "gatsby-preload-fonts -- --no-sandbox",
    "debug": "set DEBUG=* gatsby develop --host 0.0.0.0",
    "ndevelop": "netlify env:import .env.development && netlify dev",
    "develop": "cross-env NODE_ENV=development GATBY_ENV=development && gatsby develop --host 0.0.0.0",
    "develop-prod": "cross-env NODE_ENV=production GATBY_ENV=production && gatsby develop --host 0.0.0.0 --verbose",
    "build-server": "cross-env NODE_ENV=development && netlify functions:build --src netlify/functions/functions.js",
    "serve-server": "cross-env NODE_ENV=development && netlify functions:serve",
    "serve-server2": "cross-env NODE_ENV=production && netlify deploy --open",
    "builddev": "cross-env NODE_ENV=devbuild && gatsby build --log-pages --verbose",
    "build": "netlify env:import .env.production && gatsby build && netlify functions:build --src netlify/functions",
    "dev": "cross-env NODE_ENV=development pnpm run develop --host 0.0.0.0",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "npm run build && netlify deploy --prod",
    "muiV5": "npx @mui/codemod v5.0.0/jss-to-styled /src/* && npx @mui/codemod v5.0.0/preset-safe /src/*",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix src/**/*.{js,jsx,ts,tsx}",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
